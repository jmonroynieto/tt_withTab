        },
        {
            "name": "realRaw",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src",
            "console": "integratedTerminal",
            "args": ["-raw","--quotes", "test.json"],
        },
        {
            "name": "oneshot",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src",
            "console": "integratedTerminal",
            "args": ["-t", "2", "-oneshot"],
        },

    ]
